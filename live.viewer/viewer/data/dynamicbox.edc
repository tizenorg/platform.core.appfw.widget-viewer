				}
			}
		}

		programs
		{
			program {
				name: "reset,overlay";
				signal: "reset";
				source: "overlay";
				action: STATE_SET "disable,overlay" 0.0;
				target: "overlay";
			}
			program {
				name: "enable,overlay";
				signal: "enable";
				source: "overlay";
				action: STATE_SET "default" 0.0;
				target: "overlay";
				transition: LINEAR 0.15;
				after: "animation,done";
			}

			program {
				name: "disable,overlay";
				signal: "disable";
				source: "overlay";
				action: STATE_SET "disable,overlay" 0.0;
				target: "overlay";
				transition: LINEAR 0.15;
				after: "animation,done";
			}

			program {
				name: "animation,done";
				action: SIGNAL_EMIT "finished" "animation";
			}
		}
	}

