					color: 255 255 255 255;
					visible: 1;
				}
				description {
					state: "disable,overlay" 0.0;
					inherit: "default" 0.0;
					color: 0 0 0 0;
					visible: 0;
				}
			}

			part {
				name: "overlay,content";
				type: SWALLOW;
				clip_to: "overlay";
				description {
					state: "default" 0.0;
					rel1 { relative: 0.0 0.0; }
