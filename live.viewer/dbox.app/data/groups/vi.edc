///////////////////////////////////////////////////////////////////////////////////////////////
// Fade In/Out
///////////////////////////////////////////////////////////////////////////////////////////////
group {
	name: "vi/fade";
	parts {
		part {
			name: "elm.swallow.content";
			type: SWALLOW;
			clip_to: "clipper";
			description {
				rel1.relative: 0.4 0.4;
				rel2.relative: 0.6 0.6;
			}
		}
		part {
			name: "clipper";
			type: RECT;
			description {
				state: "default" 0.0;
				color: 255 255 255 100;
				rel1.to: "elm.swallow.content";
				rel2.to: "elm.swallow.content";
			}
			description {
				state: "fade_in" 0.0;
				inherit: "default";
				color: 255 255 255 255;
			}
			description {
				state: "fade_out" 0.0;
				inherit: "default";
				color: 255 255 255 0;
			}
		}
	}
	programs {
		program {
			name: "reset";
			signal: "elm,action,reset";
			source: "elm";
			action: STATE_SET "default" 0.0;
			transition: LINEAR 0.5;
			target: "clipper";
		}
		program {
			name: "show_fade_in";
			signal: "elm,action,in";
			source: "elm";
			action: STATE_SET "fade_in" 0.0;
			transition: LINEAR 0.5;
			target: "clipper";
		}
		program {
			name: "show_fade_out";
			signal: "elm,action,out";
			source: "elm";
			action: STATE_SET "fade_out" 0.0;
			transition: LINEAR 0.5;
			target: "clipper";
		}
	}
}

///////////////////////////////////////////////////////////////////////////////////////////////
// Zoom In/Out
///////////////////////////////////////////////////////////////////////////////////////////////
group {
	name: "vi/zoom";
	parts {
		part {
			name: "base";
			type: RECT;
			description {
				state: "default" 0.0;
				color: 0 0 0 0;
				perspective {
					zplane: 0;
				}
			}
		}
		part {
			name: "zoom_*1/4";
			type: RECT;
			description {
				state: "default" 0.0;
				color: 0 0 0 0;
				perspective {
					zplane: -1400;
				}
			}
	     }
		part {
			name: "zoom_*1/2";
			type: RECT;
			description {
				state: "default" 0.0;
				color: 0 0 0 0;
				perspective {
					zplane: -700;
				}
			}
	     }
		part {
			name: "zoom_*2";
			type: RECT;
			description {
				state: "default" 0.0;
				color: 0 0 0 0;
				perspective {
					zplane: 500;
				}
			}
	     }
		part {
			name: "zoom_*4";
			type: RECT;
			description {
				state: "default" 0.0;
				color: 0 0 0 0;
				perspective {
					zplane: 700;
				}
			}
	     }
		part {
			name: "elm.swallow.content";
			type: SWALLOW;
			description {
				state: "default" 0.0;
				rel1 { relative: 0.4 0.4; }
				rel2 { relative: 0.6 0.6; }
				map {
					on: 1;
					perspective: "base";
				}
			}
			description {
				state: "zoom_1/4" 0.0;
				inherit: "default";
				map {
					perspective: "zoom_*1/4";
				}
			}
			description {
				state: "zoom_1/2" 0.0;
				inherit: "default";
				map {
					perspective: "zoom_*1/2";
				}
			}
			description {
				state: "zoom_2" 0.0;
				inherit: "default";
				map {
					perspective: "zoom_*2";
				}
			}
			description {
				state: "zoom_4" 0.0;
				inherit: "default";
				map {
					perspective: "zoom_*4";
				}
			}
		}
		part {
			name: "btn1";
			type: SWALLOW;
			description {
				rel1.relative: 0.0 0.9;
				rel2.relative: 0.25 1.0;
				align: 0.5 0.5;
			}
		}
		part {
			name: "btn2";
			type: SWALLOW;
			description {
				rel1.relative: 0.25 0.9;
				rel2.relative: 0.5 1.0;
				align: 0.5 0.5;
			}
		}
		part {
			name: "btn3";
