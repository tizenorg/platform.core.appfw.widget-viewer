///////////////////////////////////////////////////////////////////////////////////////////////
// CUSTOM
///////////////////////////////////////////////////////////////////////////////////////////////
group {
	name: "vi/custom";
	parts {
		part {
			name: "in";
			type: RECT;
			description {
				state: "default" 0.0;
				rel1.to: "elm.swallow.content";
				rel2.to: "elm.swallow.content";
				color: 0 0 0 0;
				perspective {
				       zplane: 300;
				}
			}
		}
		part {
			name: "base";
			type: RECT;
			description {
				state: "default" 0.0;
				rel1.to: "elm.swallow.content";
				rel2.to: "elm.swallow.content";
				color: 0 0 0 0;
				perspective {
					zplane: 0;
				}
			}
		}
		part {
			name: "out";
			type: RECT;
			description {
				state: "default" 0.0;
				rel1.to: "elm.swallow.content";
				rel2.to: "elm.swallow.content";
				color: 0 0 0 0;
				perspective {
					zplane: -1400;
				}
			}
		}
		part {
			name: "elm.swallow.content";
			type: SWALLOW;
			scale: 1;
			clip_to: "clipper";
			description {
				state: "default" 0.0;
				rel1.relative: 0.4 0.4;
				rel2.relative: 0.6 0.6;
				map {
					on: 1;
					perspective: "base";
				}
			}
			description {
				state: "zoom_in" 0.0;
				inherit: "default";
				rel1.relative: 0.7 0.7;
				rel2.relative: 0.9 0.9;
				map {
					perspective: "in";
				}
			}
			description {
				state: "zoom_out" 0.0;
				inherit: "default";
				rel1.relative: 0.1 0.1;
				rel2.relative: 0.3 0.3;
				map {
					perspective: "out";
				}
			}
			description {
				state: "move1";
				inherit: "zoom_in";
			}
			description {
				state: "move2";
				inherit: "zoom_out";
			}
		}
		part {
			name: "clipper";
			type: RECT;
			description {
				state: "default" 0.0;
				color: 255 255 255 100;
			}
			description {
				state: "fade_in" 0.0;
				color: 255 255 255 255;
			}
			description {
				state: "fade_out" 0.0;
				color: 255 255 255 30;
			}
		}
	}
	programs {
		program {
			name: "reset";
			signal: "elm,action,reset";
			source: "elm";
			action: STATE_SET "default" 0.0;
			transition: LINEAR 0.5;
			target: "clipper";
			target: "elm.swallow.content";
		}
		program {
			name: "show_fade_in";
			signal: "elm,action,_fade_in";
			source: "elm";
			action: STATE_SET "fade_in" 0.0;
			transition: LINEAR 0.5;
			target: "clipper";
		}
		program {
			name: "show_fade_out";
			signal: "elm,action,_fade_out";
			source: "elm";
			action: STATE_SET "fade_out" 0.0;
			transition: LINEAR 0.5;
			target: "clipper";
		}
		program {
			name: "show_zoom_in";
			signal: "elm,action,_zoom_in";
			source: "elm";
			action: STATE_SET "zoom_in" 0.0;
			transition: LINEAR 0.5;
			target: "elm.swallow.content";
			after: "show_move1";
		}
		program {
			name: "show_zoom_out";
			signal: "elm,action,_zoom_out";
			source: "elm";
			action: STATE_SET "zoom_out" 0.0;
			transition: LINEAR 0.5;
			target: "elm.swallow.content";
			after: "show_move2";
		}
		program {
			name: "show_move1";
			action: STATE_SET "move1" 0.0;
			transition: LINEAR 0.5;
			target: "elm.swallow.content";
		}
		program {
			name: "show_move2";
			action: STATE_SET "move2" 0.0;
			transition: LINEAR 0.5;
			target: "elm.swallow.content";
		}
	}
}
